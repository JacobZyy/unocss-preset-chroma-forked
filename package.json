{
  "name": "unocss-preset-chroma-forked",
  "version": "0.0.1",
  "description": "chroma-js gradient for UnoCSS, a replacement of unocss-perset-chroma which is unavaliable",
  "engines": {
    "node": ">=14.18"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-demo": "vite build --mode demo",
    "test": "vitest",
    "test:update": "vitest -u",
    "release": "npx bumpp package.json --commit \"release: v\" --push --tag"
  },
  "peerDependencies": {
    "chroma-js": "2.4.2"
  },
  "devDependencies": {
    "unocss": "^0.58.5",
    "chroma-js": "2.4.2",
    "@types/estree": "^1.0.0",
    "@types/node": "^18.14.6",
    "marked": "^4.2.12",
    "typescript": "^5.3.3",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JacobZyy/unocss-preset-chroma-forked"
  },
  "keywords": [
    "unocss",
    "chroma-js",
    "gradient",
    "unocss-preset-chroma"
  ],
  "author": "Jacob",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JacobZyy/unocss-preset-chroma-forked/issues"
  },
  "homepage": "https://github.com/JacobZyy/unocss-preset-chroma-forked"
}
